<Page
    x:Class="Posta.Forms.SlanjePaketa"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Posta.Forms"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">


        <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" RenderTransformOrigin="0.372,0.506">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="5*"/>
                <ColumnDefinition Width="7*"/>
                <ColumnDefinition Width="8*"/>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="96*"/>
                <RowDefinition Height="75*"/>
                <RowDefinition Height="85*"/>
                <RowDefinition Height="93*"/>
                <RowDefinition Height="80*"/>
                <RowDefinition Height="211*"/>

            </Grid.RowDefinitions>
            <Button x:ConnectionId='1' x:Name="button" Content="Nazad" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="32" Width="62"/>
            <TextBlock x:ConnectionId='2' x:Name="textBlock" Grid.Column="1" HorizontalAlignment="Left" Margin="12,15,0,0" Grid.Row="3" TextWrapping="Wrap" Text="Datum slanja:" VerticalAlignment="Top" Width="114" RenderTransformOrigin="0.529,9.1" Height="20"/>
            <TextBox x:ConnectionId='3' x:Name="textBox" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Adresa" VerticalAlignment="Top" Width="114" Height="32"/>
            <TextBox x:ConnectionId='4' x:Name="textBox_Copy" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="2" TextWrapping="Wrap" Text="Tezina" VerticalAlignment="Top" Width="114" Height="32"/>
            <TextBox x:ConnectionId='5' x:Name="textBox_Copy1" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="4" TextWrapping="Wrap" Text="Cijena" VerticalAlignment="Top" Width="114" Height="32"/>
            <CalendarDatePicker Grid.Column="2" HorizontalAlignment="Left" Grid.Row="3" VerticalAlignment="Top" Width="126" Height="32" Margin="8,10,0,0"/>
            <Button x:ConnectionId='6' x:Name="button1" Content="REGISTRACIJA" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="5" VerticalAlignment="Top"/>
        </Grid>
    </Page>


